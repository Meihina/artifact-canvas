{
  "name": "artifact-canvas",
  "version": "0.0.7",
  "type": "module",
  "files": [
    "dist",
    "es",
    "lib",
    "src",
    "unpkg",
    "types"
  ],
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix --ext .js,.vue ./src",
      "prettier --write",
      "git add"
    ]
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./types/**/*.d.ts",
  "exports": {
    "./*": {
      "import": "./es/*/index.js",
      "require": "./lib/*/index.cjs"
    }
  },
  "scripts": {
    "start": "vite",
    "check": "tsc && npx vue-tsc --declaration --emitDeclarationOnly",
    "babel": "mkdir -p temp && npx vue-tsc --outDir temp && npx babel temp --out-dir temp",
    "clear": "rm -rf ./dist ./es ./lib ./unpkg",
    "build": "npm run clear && npm run check & npx esno ./src/shell/build.js & npx esno ./src/shell/umd.js",
    "build:main": "npm run clear && npm run check && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "localhost": "http-server dist",
    "deploy": "npm publish --registry https://registry.npmjs.org"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.26.0",
    "artifact-canvas": "0.0.6",
    "core-js": "^3.6.5",
    "glsl-noise": "^0.0.0",
    "glslCanvas": "^0.1.7",
    "qs": "^6",
    "regenerator-runtime": "^0.14.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.2"
  },
  "browserify": {
    "transform": [
      "glslify"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/node": "^20.12.12",
    "@types/qs": "6.9.17",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "eslint-plugin-vuejs-accessibility": "^1.1.0",
    "esno": "^4.8.0",
    "glslify": "^7.1.1",
    "glslify-loader": "^2.0.0",
    "husky": "^7.0.0",
    "less": "^4.2.0",
    "lint-staged": "^12.1.3",
    "prettier": "^2.7.1",
    "terser": "^5.37.0",
    "ts-node": "^10.9.2",
    "typescript": "5.6.3",
    "vite": "^5.2.0",
    "vite-plugin-babel": "^1.3.0",
    "vite-plugin-babel-import": "^2.0.5",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-glslify": "^2.1.0",
    "vite-plugin-lib-inject-css": "^2.0.1",
    "vue-tsc": "2.1.6",
    "commitlint": "19.6.0"
  }
}
